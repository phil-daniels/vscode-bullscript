{
    "name": "vscode-bullscript",
    "displayName": "Bullscript Language Support",
    "description": "Adds support for the Bullscript language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bullscript",
            "aliases": ["Bullscript", "bullscript"],
            "extensions": [".bs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bullscript",
            "scopeName": "source.bullscript",
            "path": "./syntaxes/bullscript.tmLanguage.json"
        }],
        "themes": [{
              "label": "Bullscript Dark",
              "uiTheme": "vs-dark",
              "path": "./themes/bullscript-dark-color-theme.json"
        }]
    }
}